{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node LTS (22)",
  "_version": "18.17.0",
  "compilerOptions": {
    "lib": ["ES2022"],
    "module": "node16",
    "target": "es2022",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node16",
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
